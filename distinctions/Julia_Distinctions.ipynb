   "source": [
    "@show typeof(ARGS) # global container for command-line arguments\n",
    "\n",
    "function foo()\n",
    "    flag = false\n",
    "    for i in 1:10\n",
    "        if flag\n",
    "            break\n",
    "        elseif i < 10\n",
    "            continue\n",
    "        else\n",
    "            flag = true\n",
    "        end\n",
    "    end\n",
    "    return nothing\n",
    "end\n",
    "\n",
    "try\n",
    "    error(\"this is an error\")\n",
    "catch e\n",
    "    println(e.msg)\n",
    "    rethrow()\n",
    "end\n",
    "\n",
    "; # this is just for the notebook"
